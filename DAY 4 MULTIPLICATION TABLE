#include <stdio.h>

int main()
{
    
  int a,b;
  scanf("%d %d",&a,&b);//5
  int i=1;
       while(i<=b)//2<=50
    {
        
        
         printf("%d* %d=%d\n",i ,a,i*a);//2
        
        i++; 
        }
    

    return 0;
}
 OUTPUT :2 10
1* 2=2
2* 2=4
3* 2=6
4* 2=8
5* 2=10
6* 2=12
7* 2=14
8* 2=16
9* 2=18
10* 2=20
