#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int rows;



    scanf("%d", &rows);//4


for(int a=1;a<=rows;a++) //1<=3 2<=3 3<=3 4<=3

{


    for(int b=1;b<=rows;b++) //1<=3 2<=3 3<=3


   {


        printf("*");



}



printf("\n");


}



    return 0;
}
